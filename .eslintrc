{
  "extends": ["react-app", "prettier", "react-app/jest"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", { 
      "singleQuote": true,
      "endOfLine": "auto" ,
      "printWidth": 160 
  }]
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint"
}
